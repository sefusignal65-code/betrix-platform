{
  "version": 2,
  "builds": [
    {
      "src": "pages/**/*",
      "use": "@vercel/next"
    },
    {
      "src": "frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },

    {
      "src": "/(.*)",
      "dest": "/frontend/dist/$1",
      "headers": {
        "x-content-type-options": "nosniff",
        "x-frame-options": "DENY",
        "x-xss-protection": "1; mode=block"
      }
    }
  ],
  "public": true,
  "protection": false,
  "github": {
    "enabled": true,
    "silent": true,
    "autoAlias": true
  }
}